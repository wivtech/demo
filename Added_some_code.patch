+  font-weight: bold;
+}
+
+.bolder-text {
+  font-weight: bolder;
+}
+
+.fade-gray-text {
+  color: #B2B2B2;
+}
+
+.center-text {
+  text-align: center;
+}
+
+/* ---------------------------------------------------
+    FORMS STYLE
+----------------------------------------------------- */
+
+.form-control {
+    border: 1px solid #FFF;
+    -webkit-box-shadow: 0px 0px 5px 0px rgba(173,173,173,0.6);
+      -moz-box-shadow: 0px 0px 5px 0px rgba(173,173,173,0.6);
+      box-shadow: 0px 0px 5px 0px rgba(173,173,173,0.6);
+}
+
+input[type=radio],
+input.radio {
+  float: left;
+  clear: none;
+  margin: 0.4rem 0.3rem 0 0;
+}
+
+.label-rb {
+  float: left;
+  clear: none;
+  display: block;
+  padding: 2px 1rem 0 0;
+}
+
+/* ---------------------------------------------------
+    SIDEBAR STYLE
+----------------------------------------------------- */
+
+.wrapper {
+    display: flex;
+    width: 100%;
+    align-items: stretch;
+}
+
+#sidebar {
+    min-width: 250px;
+    max-width: 250px;
+    background: #353D40;
+    color: #A0A4A6;
+    transition: all 0.3s;
+}
+
+.sidebar-header > img {
+  width: 90%;
+}
+
+#sidebar.active {
+    margin-left: -250px;
+}
+
+#sidebar .sidebar-header {
+    padding: 20px;
+}
+
+#sidebar ul.components {
+    padding: 20px 0;
+    margin: 0 5px 0 5px;
+}
+
+#sidebar ul p {
+    padding: 10px;
+}
+
+#sidebar ul li a {
+    padding: 10px;
+    font-size: 1.1em;
+    display: block;
+}
+
+#sidebar ul li a i{
+  padding: 10px;
+  font-size: 1.1em;
+}
+
+#sidebar ul li a:hover {
+    color: #FFF;
+    background: #514C43;
+    border-radius: 5px;
+    /*opacity:0.4;*/
+}
+
+#sidebar ul li.active>a,
+a[aria-expanded="true"] {
+  color: #FFF;
+  background: #514C43;
+  border-radius: 5px;
+  /*opacity:0.4;*/
+}
+
+.sidebar-sub-menu {
+    margin-left: 1.2rem;
+}
+
+a[data-toggle="collapse"] {
+    position: relative;
+}
+
+.dropdown-toggle::after {
+    display: block;
+    position: absolute;
+    top: 50%;
+    right: 20px;
+    transform: translateY(-50%);
+}
+
+ul ul a {
+    font-size: 0.9em !important;
+    padding-left: 30px !important;
+    background: #6d7fcc;
+}
+
+ul.CTAs {
+    padding: 20px;
+}
+
+ul.CTAs a {
+    text-align: center;
+    font-size: 0.9em !important;
+    display: block;
+    border-radius: 5px;
+    margin-bottom: 5px;
+}
+
+a.download {
+    background: #fff;
+    color: #7386D5;
+}
+
+a.article,
+a.article:hover {
+    background: #6d7fcc !important;
+    color: #fff !important;
+}
+
+/* ---------------------------------------------------
+    CONTENT STYLE
+----------------------------------------------------- */
+
+#nav-wrapper {
+  width: 100%;
+  padding: 0;
+  min-height: 100vh;
+  transition: all 0.3s;
+}
+
+#content {
+    width: 100%;
+    padding: 20px;
+    min-height: 100vh;
+    transition: all 0.3s;
+}
+
+/*----------------------------------------------------
+    Icons
+------------------------------------------------------ */
+
+.material-icons {
+  font-size: 0.9rem;
+  line-height: 1.5rem;
+}
+
+.icons-right{
+  float: right;
+}
+
+/*----------------------------------------------------
+    Links
+------------------------------------------------------ */
+
+.card a {
+    color: #38AECC;
+}
+
+.offer-links-row {
+    margin-top: 0.8rem;
+}
+
+
+/*----------------------------------------------------
+    Table
+------------------------------------------------------ */
+
+table {
+    width: 98%;
+    margin: 0 auto;
+}
+
+tr {
+    border-bottom: 1px solid rgba(173,173,173,0.2);
+    height: 2.5rem;
+}
+
+/*----------------------------------------------------
+    Timeline
+------------------------------------------------------ */
+
+/* Timeline holder */
+ul.timeline {
+    list-style-type: none;
+    position: relative;
+    padding-left: 1.5rem;
+    color: #FFF;
+}
+
+ /* Timeline vertical line */
+ul.timeline:before {
+    content: ' ';
+    background: #353D40;
+    display: inline-block;
+    position: absolute;
+    left: 16px;
+    width: 4px;
+    height: 100%;
+    z-index: 400;
+    border-radius: 1rem;
+}
+
+li.timeline-item {
+    margin: 20px 0;
+}
+
+/* Timeline item arrow */
+.timeline-arrow {
+    border-top: 0.5rem solid transparent;
+    border-right: 0.5rem solid #fff;
+    border-bottom: 0.5rem solid transparent;
+    display: block;
+    position: absolute;
+    left: 2rem;
+}
+
+.timeline-arrow-blue {
+    border-right: 0.5rem solid #38AECC;
+}
+
+.timeline-arrow-orange {
+    border-right: 0.5rem solid #FF9900;
+}
+
+.timeline-arrow-green {
+    border-right: 0.5rem solid #1EC88A;
+}
+
+/* Timeline item circle marker */
+li.timeline-item::before {
+    content: ' ';
+    background: #ddd;
+    display: inline-block;
+    position: absolute;
+    border-radius: 50%;
+    border: 3px solid #fff;
+    left: 11px;
+    width: 14px;
+    height: 14px;
+    z-index: 400;
+    box-shadow: 0 0 5px rgba(0, 0, 0, 0.2);
+}
+
+li.timeline-item-blue::before {
+    background: #38AECC;
+}
+
+li.timeline-item-orange::before {
+    background: #FF9900;
+}
+
+li.timeline-item-green::before {
+    background: #1EC88A;
+}
+
+
+
+/* ---------------------------------------------------
+    MEDIAQUERIES
+----------------------------------------------------- */
+
+@media (max-width: 768px) {
+    #sidebar {
+        margin-left: -250px;
+    }
+    #sidebar.active {
+        margin-left: 0;
+    }
+    #sidebarCollapse span {
+        display: none;
+    }
+
+    .navbar {
+      background: #353D40;
+    }
+
+    .navbar-brand, .wiv-navbar-menu {
+        display: block;
+    }
+
+}
